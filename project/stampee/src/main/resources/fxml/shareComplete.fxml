<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>

<StackPane xmlns:fx="http://javafx.com/fxml" fx:controller="controller.ShareCompleteController"
           styleClass="root" prefWidth="250" prefHeight="200" stylesheets="/css/shareComplete.css">
    <VBox alignment="CENTER" spacing="20">
        <Label fx:id="messageLabel" styleClass="success-message"/>
        <Button text="확인" onAction="#handleClose" styleClass="confirm-button"/>
    </VBox>
</StackPane>